function mostOften(){
   const characters={}
   for (let char of str){
     characters[char]=characters[char]+1 || 1
   
   }
   let mostCount=0
   let mostCharacter=null
   for(let char in characters){
     if(characters[char]>mostCount){
        mostCount=characters[char]
        mostCharacter=char
     }
   }
   return mostCharacter
   
   }
